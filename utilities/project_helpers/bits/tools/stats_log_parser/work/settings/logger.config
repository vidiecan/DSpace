[loggers]
keys: common, root, utils, server

[formatters]
keys: detailedthreaded
 
[handlers]
keys = file, consolethreaded

[logger_root]
level = INFO
handlers = file, consolethreaded

[logger_server]
level = INFO
handlers = file, consolethreaded
qualname = server
propagate = 0

[logger_common]
level = INFO
handlers = file, consolethreaded
qualname = common
propagate = 0

[logger_utils]
level = INFO
handlers =  file, consolethreaded
qualname = utils
propagate = 0

[formatter_detailedthreaded]
format = "%(threadName)s:%(asctime)s,%(msecs)03d %(levelname)-5.4s [%(name)s] %(message)s
datefmt = %Y-%m-%d %H:%M:%S

[handler_consolethreaded]
class = StreamHandler
args = []
formatter=detailedthreaded

[handler_file]
class = FileHandler
args = ('logs/parse.logs'+time.strftime("-%Y-%m-%d-%H.%M.%S")+'.log', )
level = INFO
formatter = detailedthreaded
